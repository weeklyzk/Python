class Stack:
    def __init__(self):
        self.stack = []
    def is_empty(self):
        if self.stack == []:
            return True
        else: return False
    def push(self,n):
        self.stack.append(n)
    def peek(self):
        return self.stack[-1]
    def size(self):
        return len(self.stack)
    def pop(self):
        self.stack.pop()
myStack = Stack()
myStack.push(1)
myStack.push("whoeny")
print(myStack.stack, myStack.is_empty(), myStack.peek(),myStack.size())
